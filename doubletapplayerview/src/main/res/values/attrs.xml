<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Forward">
        <attr name="f_textSize" format="dimension" />
        <attr name="f_color" format="color" />
        <attr name="f_strokeWidth" format="dimension" />
        <attr name="f_textInput" format="integer" />
        <attr name="f_animationDuration" format="integer" />
        <attr name="f_scalePercent" format="integer" />
        <attr name="f_arcRotationAngle" format="float" />
        <attr name="f_sweepAngle" format="float" />
        <attr name="f_arrowSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="Rewind">
        <attr name="r_textSize" format="dimension" />
        <attr name="r_color" format="color" />
        <attr name="r_strokeWidth" format="dimension" />
        <attr name="r_textInput" format="integer" />
        <attr name="r_animationDuration" format="integer" />
        <attr name="r_scalePercent" format="integer" />
        <attr name="r_arcRotationAngle" format="float" />
        <attr name="r_sweepAngle" format="float" />
        <attr name="r_arrowSize" format="dimension" />
    </declare-styleable>
</resources>